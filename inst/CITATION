citHeader("To cite package 'DescToolsAddIns' in publications use:")

year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
vers <- paste("R package version", meta$Version)

citEntry(entry="Manual",
         title = "DescToolsAddIns: Interactive Functions to be Used as Shortcuts in 'RStudio'",
         author = personList(c("Andri Signorell")),
         year = year,
         note = vers,
         url = "https://cran.r-project.org/package=DescToolsAddIns",

         textVersion =
         paste("Andri Signorell (",
               year,
               "). DescToolsAddIns: Interactive Functions to be Used as Shortcuts in 'RStudio'. ",
               vers, ".", sep=""))




